{
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://fullstack-kanban-backend.vercel.app/api/:path*"
    }
  ]
}